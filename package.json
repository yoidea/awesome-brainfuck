{
  "name": "awesome-brainfuck",
  "version": "1.0.0",
  "author": "@yoidea",
  "dependencies": {
    "jsbrainfuck": "^0.0.4",
    "nodemon": "^2.0.1"
  },
  "scripts": {
    "start": "nodemon",
    "build": "node compile.js"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "ext": "bf",
    "exec": "node ./run.js ./src/*.bf"
  }
}
